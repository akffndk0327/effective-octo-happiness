<tiles-definitions>
   <definition name="super" template="/WEB-INF/views/layer.jsp">
      <put-attribute name="preScript" value="/includee/preScript.jsp"/>
      <put-attribute name="topMenu" value="/includee/header.jsp"/>
      <put-attribute name="leftMenu" value="/includee/Memleft.jsp"/>
      <put-attribute name="footer" value="/includee/footer.jsp"/>
   </definition>
   
	<definition name="weather/*" extends="super">
      <put-attribute name="contents" value="/WEB-INF/views/weather/{1}.jsp" />
   </definition>
   
	<definition name="diet/*" extends="super">
      <put-attribute name="contents" value="/WEB-INF/views/diet/{1}.jsp" />
   </definition>

</tiles-definitions>       