@charset "UTF-8";


.adminleft {
	width: 200px;
    height: 430px;
    background: #FAFAFA;
    font-size: 20px;
    padding: 5px;
    display: inline-block;
    float: left;
    margin-left: 18px;
}

ul {
	list-style: none;
}

li {
	list-style: none;
}

#adminTitle a {
	color: black;
}

#adminTitle {
	font-size: 26px;
	font-weight: bold;
}

#adminTitle ul li a:link, :visited, :hover {
	color: black;
	text-decoration: none;
}