 #memleft {
   width: 220px;
    height: 500px;
    background: #FAFAFA;
    font-size: 22px;
    padding: 7px;
    display: inline-block;
    float: left;
    margin-left: 50px;
}

ul {
   list-style: none;
}

li {
   list-style: none;
   padding: 5px;
}

#memleft a {
   color: black;
}

#MemTitle {
   font-size: 33px;
   font-weight: bold;
}

#memleft ul li a:link, :visited, :hover {
   color: black;
   text-decoration: none;
} 