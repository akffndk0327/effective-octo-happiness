#InnerContainer{
	margin : auto;
	margin-bottom: 70px;
}
.titleLeftBar{
	margin-left : 50px;
	padding-left : 15px;
}
table{
	margin : auto;
}
#mainTable{
	width : 83%;
	min-height : 400px;	
}
#mainTable th{
	height : 30px;
}
#mainTable td{
		font-size : 16px;
		vertical-align : middle;
}
.btnGreen{

	background: #322C5C;
	color: #fff;
	font-size: 11px;
}
.btnWhite{
	background: #fff;
	color: #322C5C;
	font-size: 11px;
	border: 1px solid #322C5C;
}
th, td{
	text-align : center;
}
.leftTd{
	text-align : left;
	width:35%;
}
.btnLong{
	width : 100px;
}
.SearchT{
	border : 0px;
}
.SearchT input {
	width : 250px;
	height : 30px;
	padding-left : 10px;
}
.SearchT select {
	width : 100px;
	height: 36px;
	padding-left : 10px;
}
.SearchT a, #search{
	cursor : pointer;
	font-size : 15px;
}
.SearchT a:hover{
	background: #322C5C;
	color: #fff;
}
.blank{
	width : 48%;
}
.blank2{
	width : 38%;
}
.table a{
	cursor : pointer;
}
#inputReason{
	width : 300px;
	height : 100px;
}
.modal-title{
	font-size : 20px;
	font-weight : bold;
}
.modal-dialog { 
	width: 23%; height: 30%;
}

.prodImg{
	width : 100px;
	height : 100px;
}
#pagingArea{
	text-align : center;
}
.titleLeftBar{
	border-left: 8px solid #322C5C;
	color: #322C5C;
}

.pagination>.active>a, .pagination>.active>span, .pagination>.active>a:hover, .pagination>.active>span:hover, .pagination>.active>a:focus, .pagination>.active>span:focus {
    z-index: 2;
    color: #fff;
    cursor: default;
    background-color: #322C5C;
    border-color: #322C5C;
}
.nameURL{
	cursor : pointer;
}
.nameURL:hover{
	color : blue;
	text-decoration: underline;
	cursor : pointer;
}

