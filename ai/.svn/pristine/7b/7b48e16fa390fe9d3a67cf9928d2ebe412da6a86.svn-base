@charset "UTF-8";

#comleft {
	width: 150px;
    height: 500px;
    background: #FAFAFA;
    font-size: 20px;
    padding: 7px;
    display: inline-block;
    float: left;
    margin-left: 18px;
}

ul {
	list-style: none;
}

li {
	list-style: none;
}

#comleft a {
	color: black;
}

#ComTitle {
	font-size: 26px;
	font-weight: bold;
}

#comleft ul li a:link, :visited, :hover {
	color: black;
	text-decoration: none;
}